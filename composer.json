{
    "name": "rubix/boards",
    "description": "An extension to the Rubix graph engine for solving board games.",
    "license": "MIT",
    "keywords": ["php", "graph", "boggle", "scrabble"],
    "authors": [
        {
            "name": "Andrew DalPino",
            "email": "me@andrewdalpino.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "league/climate": "^3.2",
        "rubix/engine": "^0.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "6.0.*"
    },
    "autoload": {
        "psr-4": {
            "Rubix\\Boards\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
